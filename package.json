{
  "name": "matkit",
  "version": "0.0.1",
  "description": "ui-kit for material design",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "scripts": {
    "typecheck:play": "tsc -p tsconfig.json --noEmit",
    "dev:play": "NODE_ENV=development webpack serve --config ./webpack.play.config.cjs",
    "build:play": "NODE_ENV=production webpack --config ./webpack.play.config.cjs",
    "start:play": "serve -s playground-dist -l 3000",
    "clean:lib": "rm -rf dist",
    "build:icons": "svgr -d src/icons assets/icons",
    "build-esm:lib": "swc src/* -d dist --strip-leading-paths --config-file .esm.swcrc --out-file-extension mjs",
    "build-cjs:lib": "swc src/* -d dist --strip-leading-paths --config-file .cjs.swcrc --out-file-extension cjs",
    "build-ts:lib": "npm run build-esm:lib && npm run build-cjs:lib && tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "copy-css:lib": "cpy \"src/**/*.css\" dist --parents",
    "build:lib": "npm run clean:lib && npm run build:icons && npm run build-ts:lib && npm run copy-css:lib"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webpack-env": "^1.18.8",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "serve": "^14.2.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.mjs",
      "require": "./dist/components/*/index.cjs"
    },
    "./icons/*": {
      "types": "./dist/icons/*/index.d.ts",
      "import": "./dist/icons/*/index.mjs",
      "require": "./dist/icons/*/index.cjs"
    }
  }
}
