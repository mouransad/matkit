{
  "name": "matkit",
  "version": "0.0.1",
  "description": "ui-kit for material design",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "scripts": {
    "typecheck:play": "tsc -p tsconfig.json --noEmit",
    "dev:play": "NODE_ENV=development webpack serve --config ./webpack.play.config.cjs",
    "build:play": "NODE_ENV=production webpack --config ./webpack.play.config.cjs",
    "start:play": "serve -s playground-dist -l 3000",
    "build:lib": "webpack --config ./webpack.lib.esm.config.cjs && tsc -p tsconfig.lib.json"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webpack-env": "^1.18.8",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "serve": "^14.2.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "sideEffects": [
    "*.css",
    "**/*.css"
  ],
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "files": ["dist"],
  "exports": {
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.mjs",
      "require": "./dist/components/*/index.js"
    },
    "./button.css": "./dist/assets/css/button.css"
  }
}
