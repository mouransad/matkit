{
  "jsc": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/lib/components/*"],
      "@icons/*": ["src/lib/icons/*"],
      "@lib/*": ["src/lib/*"]
    },
    "target": "es2018",
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "importSource": "react"
      }
    }
  },
  "module": {
    "type": "es6"
  }
}
